{
  "name": "simple-plist",
  "author": "Joe Wollard",
  "license": "MIT",
  "homepage": "https://github.com/wollardj/node-simple-plist.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/wollardj/node-simple-plist.git"
  },
  "version": "1.1.0",
  "description": "A wrapper utility for interacting with plist data.",
  "main": "dist/simple-plist.js",
  "types": "dist/simple-plist.d.ts",
  "files": [
    "dist/"
  ],
  "keywords": [
    "plist",
    "binary",
    "bplist",
    "xml"
  ],
  "scripts": {
    "build:ts": "tsc",
    "build": "run-s clean build:ts",
    "clean": "rimraf dist __tests__/write-test* coverage",
    "jest:verbose": "jest --coverage --verbose",
    "jest": "jest --coverage ",
    "prepare": "run-s clean build:ts",
    "test:verbose": "run-s clean jest:verbose",
    "test": "run-s clean jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx run-s test",
      "pre-push": "npx run-s test"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "bplist-creator": "0.0.8",
    "bplist-parser": "0.2.0",
    "plist": "^3.0.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/plist": "^3.0.2",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.3",
    "typescript-eslint": "^0.0.1-alpha.0"
  }
}
